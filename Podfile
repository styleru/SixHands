  pod "SwiftyJSON"
  pod 'Alamofire', '~> 4.3'
  # Pods for sixHands

  target 'sixHandsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'sixHandsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
